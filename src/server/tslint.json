{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "semicolon": [
      true,
      "never"
    ],
    "space-before-function-paren": [
      true,
      "always"
    ],
    "only-arrow-functions": [
      false
    ],
    "ban-types": [
      false
    ],
    "no-namespace": [
      false
    ],
    "object-literal-sort-keys": false,
    "ordered-imports": [
      false
    ],
    "no-var-requires": false
  },
  "rulesDirectory": []
}
